{
	"name": "dataflow5_ipl",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText17",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "Orc1",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "filter1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          id as integer,",
				"          season as string,",
				"          city as string,",
				"          date as date,",
				"          match_type as string,",
				"          player_of_match as string,",
				"          venue as string,",
				"          team1 as string,",
				"          team2 as string,",
				"          toss_winner as string,",
				"          toss_decision as string,",
				"          winner as string,",
				"          result as string,",
				"          result_margin as string,",
				"          target_runs as string,",
				"          target_overs as string,",
				"          super_over as boolean,",
				"          method as string,",
				"          umpire1 as string,",
				"          umpire2 as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 select(mapColumn(",
				"          id,",
				"          season,",
				"          city,",
				"          team1,",
				"          team2,",
				"          winner,",
				"          target_runs",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 filter(lesser(100, 200)) ~> filter1",
				"filter1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}