{
	"name": "transform csv",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "dataflow_csv",
						"type": "DatasetReference"
					},
					"name": "sourceCSV"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "dataflow_sink",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "selectcolu"
				},
				{
					"name": "filterRows"
				},
				{
					"name": "split1"
				},
				{
					"name": "removednull"
				},
				{
					"name": "aggregate"
				},
				{
					"name": "aggregateEngg"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {First Name} as string,",
				"          Gender as string,",
				"          {Start Date} as date,",
				"          {Last Login Time} as string,",
				"          Salary as integer,",
				"          {Bonus %} as double,",
				"          {Senior Management} as boolean,",
				"          Team as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourceCSV",
				"sourceCSV select(mapColumn(",
				"          {First Name},",
				"          Gender,",
				"          Salary,",
				"          {Senior Management},",
				"          Team",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selectcolu",
				"selectcolu filter(Salary > 25000) ~> filterRows",
				"removednull split(Team == 'Engineering',",
				"     Team == 'Sales',",
				"     disjoint: false) ~> split1@(Engineering, Sales, OtherThanEngg)",
				"filterRows derive(Gender = coalesce(Gender, 'N/A'),",
				"          {First Name} = coalesce({First Name}, 'N/A')) ~> removednull",
				"split1@OtherThanEngg aggregate(groupBy(Team),",
				"     Salary = max(Salary)) ~> aggregate",
				"split1@Engineering aggregate(groupBy(Team),",
				"     Salary = min(Salary)) ~> aggregateEngg",
				"aggregate sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
			]
		}
	}
}